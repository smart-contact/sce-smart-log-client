{
    "name": "smart-contact/smart-log-client",
    "type": "library",
    "require": {
        "illuminate/support": "^8.80",
        "guzzlehttp/guzzle": "^7.4",
        "monolog/monolog": "^2.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "mockery/mockery": "^1.4",
        "orchestra/testbench": "^6.24",
        "sempro/phpunit-pretty-print": "^1.4"
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "authors": [
        {
            "name": "Federico Mameli",
            "email": "federico.mamemli@smart-contact.it"
        }
    ],
    "autoload": {
