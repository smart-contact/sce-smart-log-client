{
    "name": "smart-contact/sce-smart-log-client",
    "type": "library",
    "require": {
        "illuminate/support": "^8.80",
        "guzzlehttp/guzzle": "^7.4",
        "monolog/monolog": "^2.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "mockery/mockery": "^1.4",
        "orchestra/testbench": "^6.24",
        "sempro/phpunit-pretty-print": "^1.4",
        "marcocesarato/php-conventional-changelog": "^1.13"
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "changelog": "conventional-changelog",
        "release": "conventional-changelog --commit",
        "release:patch": "conventional-changelog --patch --commit",
        "release:minor": "conventional-changelog --minor --commit",
        "release:major": "conventional-changelog --major --commit"
    },
    "authors": [
        {
            "name": "Federico Mameli",
            "email": "federico.mamemli@smart-contact.it"
        }
    ],
    "autoload": {
        "psr-4": {
            "SmartContact\\SmartLogClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SmartContact\\SmartLogClient\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SmartContact\\SmartLogClient\\SmartLogClientServiceProvider"
            ]
        }
    }
}